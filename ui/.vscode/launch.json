{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch via NPM",
        "request": "launch",
        "env": {
          "PORT": "5000",
          "REACT_APP_GRAPH_SERVER": "http://localhost:7071/api/graphql"
        },
        "console": "integratedTerminal",
        "runtimeArgs": ["start", "--inspect"],
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/react-scripts",
        "skipFiles": ["<node_internals>/**"],
        "type": "pwa-node"
      }
    ]
  }
  